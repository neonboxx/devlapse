{
  "name": "Dev-Lapse",
  "version": "0.0.2",
  "key":"dkeekkkmghhfidchildgjpdgodidjkmi",
  "manifest_version": 2,
  "description": "Log your web development in images",
  "homepage_url": "http://www.senior.co.uk",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js",
      "/js/jquery/jquery.min.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Dev-Lapse",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "<all_urls>",
    "activeTab",
    "identity",
    "https://api.imgur.com/oauth2/*",
    "https://api.imgur.com/*"
  ]
}